{
	"authors": [
		{
			"name": "Khalifah K. Shabazz",
			"homepage": "http://www.kshabazz.net",
			"role": "Developer"
		}
	],
	"autoload": {
		"psr-4": {
			"Kshabazz\\Slib\\": "src"
		}
	},
	"autoload-dev": {
		"psr-4": {
			"Kshabazz\\Slib\\Tests\\": "test/Slib"
		}
	},
	"description": "A collection of useful functions to aid developing.",
	"name": "kshabazz/slib",
	"require": {
		"guzzlehttp/guzzle": "^6.0",
		"php": ">=7.2",
		"psr/log": "^1.0",
		"psr/http-message": "^1.0"
	},
	"require-dev": {
		"phpmd/phpmd": "^2.0",
		"phpstan/phpstan": "^0.10.3",
		"phpunit/phpunit": "^7.0",
		"squizlabs/php_codesniffer": "^2.0"
	},
	"scripts": {
		"units": "phpunit",
		"pcs": "./vendor/bin/phpcs -p --standard=PSR2 --severity=8 --colors src",
		"sami": "sami.phar update ./sami-config.php"
	},
	"suggest": {
		"ext-pdo": "*"
	}
}